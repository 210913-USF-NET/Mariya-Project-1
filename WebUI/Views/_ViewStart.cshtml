@model Models.Customer

@{
    
    @if (Model == null)
        {
        Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
        }

    else if (Model.UserName.Equals("Admin"))
        {
        Layout = "~/Views/Shared/_Layout.cshtml";
        }
    else
        {
        Layout = "~/Views/Shared/_LayoutUse.cshtml";
        }
}