@model IEnumerable<Models.StoreFront>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <caption>List of Stores</caption>
    <thead>
        <tr>
  
            <th scope="col">
                @Html.DisplayNameFor(model => model.StoreName)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.StoreStreet)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.StoreCity)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.StoreState)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.StoreCountry)
            </th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>


            <td>
                @Html.DisplayFor(modelItem => item.StoreName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StoreStreet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StoreCity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StoreState)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StoreCountry)
            </td>
            <form>
                <input type="hidden" class="form-control" value="@item.StoreFrontId" />
            </form>
            <td>
                @Html.ActionLink("Inventory", "Index", "Inventory", new { id = item.StoreFrontId }) |
                @Html.ActionLink("Order", "Index", "Order", new { id = item.StoreFrontId }) |
                @Html.ActionLink("Edit", "Edit", new { id = item.StoreFrontId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.StoreFrontId })
            </td>
        </tr>
}
    </tbody>
</table>
